{
  "name": "timestamp",
  "version": "1.0.0",
  "description": "timestamp api service",
  "private": true,
  "engines": {
    "node": "^14.0.0",
    "npm": "^6.14.4"
  },
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "tsc -p ./tsconfig.build.json",
    "build:dev": "tsc",
    "checkversions": "node ./scripts/checkversions.cjs",
    "start:dev": "cross-env NODE_ENV=development node ./dist/server.js",
    "predev": "npm run checkversions",
    "dev": "nodemon --exec run-s build:dev start:dev",
    "prestart": "npm run checkversions",
    "start": "cross-env NODE_ENV=production node ./dist/server.js",
    "test": "tsc -p ./tsconfig.test.json"
  },
  "keywords": [
    "node",
    "api",
    "express"
  ],
  "author": "Fabrizio A. Vitale",
  "license": "MIT",
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/lodash": "^4.14.155",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "eslint": "^7.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "check-node-version": "^4.0.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "tiny-invariant": "^1.1.0"
  }
}
