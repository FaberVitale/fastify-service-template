{
  "name": "timestamp",
  "version": "1.0.0",
  "description": "timestamp api service",
  "private": true,
  "engines": {
    "node": "^14.0.0",
    "npm": "^6.14.4"
  },
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "tsc -p ./tsconfig.build.json",
    "build:dev": "tsc",
    "checkversions": "node ./scripts/checkversions.cjs",
    "start:dev": "cross-env NODE_ENV=development node ./dist/server.js",
    "start:dev:debug": "cross-env NODE_ENV=development node --inspect ./dist/server.js",
    "predev": "npm run checkversions",
    "dev": "nodemon --exec run-s build:dev start:dev",
    "predev:debug": "npm run checkversions",
    "dev:debug": "nodemon --exec run-s build:dev start:dev:debug",
    "prestart": "npm run checkversions",
    "start": "cross-env NODE_ENV=production node ./dist/server.js",
    "lint:ts": "tsc -p ./tsconfig.test.json",
    "test": "jest"
  },
  "keywords": [
    "node",
    "api",
    "fastify",
    "docker"
  ],
  "author": "Fabrizio A. Vitale",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.5",
    "@types/lodash": "^4.14.157",
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "babel-jest": "^26.1.0",
    "eslint": "^7.5.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "prettier": "2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.10.1",
    "ajv-errors": "^1.0.1",
    "check-node-version": "^4.0.3",
    "cross-env": "^7.0.2",
    "fastify": "^3.1.1",
    "fastify-accepts-serializer": "^3.0.1",
    "fastify-compress": "^3.2.1",
    "fastify-cors": "^4.1.0",
    "fastify-etag": "^1.0.0",
    "fastify-swagger": "^3.1.2",
    "lodash": "^4.17.19",
    "npm-run-all": "^4.1.5",
    "tiny-invariant": "^1.1.0",
    "utility-types": "^3.10.0"
  }
}
